template#qrViewModal
  .modal(:class="{'is-active' : isOpen}")
    .modal-background
    .modal-card.qr-wrapper
      .box
        .block.qr-code
          canvas#qrCode
        .box-foot
          a.button.is-primary(@click='download')
            i.fa.fa-download
            span 儲存
          a.button(@click='isOpen = false') 取消
